<h1 style="color: red">Historial</h1>

<% if(productos.length > 0){%>
<table class="table">
  <tr style="color: yellow">
    <th>title</th>
    <th>price</th>
    <th>thumbnail</th>
  </tr>
  <% productos.forEach(producto=>{%>
  <tr>
    <td><%= producto.title%></td>
    <td><%= producto.apellido%></td>
    <td><%= producto.thumbnail%></td>
  </tr>
  <%})%>
</table>
<% } else %>
<h2 style="color: blue">No se encontraron datos</h2>
<%}%>
